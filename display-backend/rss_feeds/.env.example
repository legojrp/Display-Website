# Example environment configuration for Gemini AI Article Ranker
# Copy this to .env and fill in your actual API key

# Gemini API Key - Get this from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Database configuration
DB_PATH=articles.db

# Rate limiting configuration (optional - defaults are for free tier)
GEMINI_REQUESTS_PER_MINUTE=10
GEMINI_TOKENS_PER_MINUTE=250000
GEMINI_REQUESTS_PER_DAY=250
GEMINI_BATCH_SIZE=5
